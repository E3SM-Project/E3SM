  <comp_archive_spec compname="cice" compclass="ice">
    <rest_file_extension>\.[ri].*</rest_file_extension>
    <hist_file_extension>\.h.*.nc$</hist_file_extension>
    <rest_history_varname>unset</rest_history_varname>
