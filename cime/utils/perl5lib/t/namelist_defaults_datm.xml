<strm_domfil     stream="CLM1PT.CLM_USRDAT">domain.lnd.1x1pt-$CLM_USRDAT_NAME.nc</strm_domfil>
<strm_datdir     stream="CLM1PT.CLM_USRDAT">$DIN_LOC_ROOT/CLM1PT_data/$CLM_USRDAT_NAME</strm_datdir>
<strm_datfil     stream="CLM1PT.CLM_USRDAT">clm1pt${CLM_USRDAT_NAME}_%ym.nc</strm_datfil>
