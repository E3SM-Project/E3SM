<!-- =========================================  -->
<!--- dlnd.sno.cplhist                          -->
<!-- These dlnd values are included here for    -->
<!-- the sake of testing %glc substitution      -->
<!-- =========================================  -->

<strm_year_align stream="sno.cplhist">0001</strm_year_align>
<strm_year_start stream="sno.cplhist">1972</strm_year_start>
<strm_year_end   stream="sno.cplhist">1973</strm_year_end>
<strm_cpl_case   stream="sno.cplhist">mycase</strm_cpl_case>
<strm_offset     stream="sno.cplhist">0</strm_offset>
<strm_tintalgo   stream="sno.cplhist">lower</strm_tintalgo>
<strm_taxmode    stream="sno.cplhist">cycle</strm_taxmode>
<strm_domdir     stream="sno.cplhist">$DIN_LOC_ROOT/lnd/dlnd7</strm_domdir>
<strm_domfil     stream="sno.cplhist">domain.lnd.fv0.9x1.25_gx1v6.090309.nc</strm_domfil>
<strm_datdir     stream="sno.cplhist">mydir</strm_datdir>
<strm_datfil     stream="sno.cplhist">mycase.cpl.hs2x.%y-01-01.nc</strm_datfil>
<strm_domvar     stream="sno.cplhist">
  time    time
  xc      lon
  yc      lat
  area    area
  mask    mask
</strm_domvar>
<strm_datvar     stream="sno.cplhist">
  s2x_Ss_tsrf%glc     tsrf%glc
  s2x_Ss_topo%glc     topo%glc
  s2x_Fgss_qice%glc   qice%glc
</strm_datvar>


