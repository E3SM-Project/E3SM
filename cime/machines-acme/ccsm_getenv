#!/bin/csh -f

set xmlquery_data=`./xmlquery -s JGFSEP CASEROOT CASE CASETOOLS CCSMROOT CIMEROOT LIBROOT EXEROOT OBJROOT INCROOT SHAREDLIBROOT CASEBUILD DIN_LOC_ROOT RUNDIR LOGDIR MACH GMAKE GMAKE_J NINST_BUILD NINST_VALUE SMP_BUILD SMP_VALUE BUILD_THREADED BUILD_STATUS COMP_INTERFACE USE_ESMF_LIB COMPILER MPILIB DEBUG OS MAX_TASKS_PER_NODE PES_PER_NODE COST_PES CCSM_ESTCOST PROJECT COMP_CPL COMP_ATM COMP_LND COMP_ICE COMP_OCN COMP_GLC COMP_WAV COMP_ROF NTASKS_CPL NTASKS_ATM NTASKS_LND NTASKS_ICE NTASKS_OCN NTASKS_GLC NTASKS_WAV NTASKS_ROF NTHRDS_CPL NTHRDS_ATM NTHRDS_LND NTHRDS_ICE NTHRDS_OCN NTHRDS_GLC NTHRDS_WAV NTHRDS_ROF ROOTPE_CPL ROOTPE_ATM ROOTPE_LND ROOTPE_ICE ROOTPE_OCN ROOTPE_GLC ROOTPE_WAV ROOTPE_ROF PSTRID_CPL PSTRID_ATM PSTRID_LND PSTRID_ICE PSTRID_OCN PSTRID_GLC PSTRID_WAV PSTRID_ROF NINST_ATM NINST_LND NINST_ICE NINST_OCN NINST_GLC NINST_WAV NINST_ROF PROFILE_PAPI_ENABLE`
set CASEROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $1}'`
set CASE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $2}'`
set CASETOOLS=`echo $xmlquery_data | awk -F'JGFSEP' '{print $3}'`
set CCSMROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $4}'`
set CIMEROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $5}'`
set LIBROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $6}'`
set EXEROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $7}'`
set OBJROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $8}'`
set INCROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $9}'`
set SHAREDLIBROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $10}'`
set CASEBUILD=`echo $xmlquery_data | awk -F'JGFSEP' '{print $11}'`
set DIN_LOC_ROOT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $12}'`
set RUNDIR=`echo $xmlquery_data | awk -F'JGFSEP' '{print $13}'`
set LOGDIR=`echo $xmlquery_data | awk -F'JGFSEP' '{print $14}'`
set MACH=`echo $xmlquery_data | awk -F'JGFSEP' '{print $15}'`
set GMAKE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $16}'`
set GMAKE_J=`echo $xmlquery_data | awk -F'JGFSEP' '{print $17}'`
set NINST_BUILD=`echo $xmlquery_data | awk -F'JGFSEP' '{print $18}'`
set NINST_VALUE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $19}'`
set SMP_BUILD=`echo $xmlquery_data | awk -F'JGFSEP' '{print $20}'`
set SMP_VALUE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $21}'`
set BUILD_THREADED=`echo $xmlquery_data | awk -F'JGFSEP' '{print $22}'`
set BUILD_STATUS=`echo $xmlquery_data | awk -F'JGFSEP' '{print $23}'`
set COMP_INTERFACE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $24}'`
set USE_ESMF_LIB=`echo $xmlquery_data | awk -F'JGFSEP' '{print $25}'`
set COMPILER=`echo $xmlquery_data | awk -F'JGFSEP' '{print $26}'`
set MPILIB=`echo $xmlquery_data | awk -F'JGFSEP' '{print $27}'`
set DEBUG=`echo $xmlquery_data | awk -F'JGFSEP' '{print $28}'`
set OS=`echo $xmlquery_data | awk -F'JGFSEP' '{print $29}'`
set MAX_TASKS_PER_NODE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $30}'`
set PES_PER_NODE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $31}'`
set COST_PES=`echo $xmlquery_data | awk -F'JGFSEP' '{print $32}'`
set CCSM_ESTCOST=`echo $xmlquery_data | awk -F'JGFSEP' '{print $33}'`
set PROJECT=`echo $xmlquery_data | awk -F'JGFSEP' '{print $34}'`
set COMP_CPL=`echo $xmlquery_data | awk -F'JGFSEP' '{print $35}'`
set COMP_ATM=`echo $xmlquery_data | awk -F'JGFSEP' '{print $36}'`
set COMP_LND=`echo $xmlquery_data | awk -F'JGFSEP' '{print $37}'`
set COMP_ICE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $38}'`
set COMP_OCN=`echo $xmlquery_data | awk -F'JGFSEP' '{print $39}'`
set COMP_GLC=`echo $xmlquery_data | awk -F'JGFSEP' '{print $40}'`
set COMP_WAV=`echo $xmlquery_data | awk -F'JGFSEP' '{print $41}'`
set COMP_ROF=`echo $xmlquery_data | awk -F'JGFSEP' '{print $42}'`
set NTASKS_CPL=`echo $xmlquery_data | awk -F'JGFSEP' '{print $43}'`
set NTASKS_ATM=`echo $xmlquery_data | awk -F'JGFSEP' '{print $44}'`
set NTASKS_LND=`echo $xmlquery_data | awk -F'JGFSEP' '{print $45}'`
set NTASKS_ICE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $46}'`
set NTASKS_OCN=`echo $xmlquery_data | awk -F'JGFSEP' '{print $47}'`
set NTASKS_GLC=`echo $xmlquery_data | awk -F'JGFSEP' '{print $48}'`
set NTASKS_WAV=`echo $xmlquery_data | awk -F'JGFSEP' '{print $49}'`
set NTASKS_ROF=`echo $xmlquery_data | awk -F'JGFSEP' '{print $50}'`
set NTHRDS_CPL=`echo $xmlquery_data | awk -F'JGFSEP' '{print $51}'`
set NTHRDS_ATM=`echo $xmlquery_data | awk -F'JGFSEP' '{print $52}'`
set NTHRDS_LND=`echo $xmlquery_data | awk -F'JGFSEP' '{print $53}'`
set NTHRDS_ICE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $54}'`
set NTHRDS_OCN=`echo $xmlquery_data | awk -F'JGFSEP' '{print $55}'`
set NTHRDS_GLC=`echo $xmlquery_data | awk -F'JGFSEP' '{print $56}'`
set NTHRDS_WAV=`echo $xmlquery_data | awk -F'JGFSEP' '{print $57}'`
set NTHRDS_ROF=`echo $xmlquery_data | awk -F'JGFSEP' '{print $58}'`
set ROOTPE_CPL=`echo $xmlquery_data | awk -F'JGFSEP' '{print $59}'`
set ROOTPE_ATM=`echo $xmlquery_data | awk -F'JGFSEP' '{print $60}'`
set ROOTPE_LND=`echo $xmlquery_data | awk -F'JGFSEP' '{print $61}'`
set ROOTPE_ICE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $62}'`
set ROOTPE_OCN=`echo $xmlquery_data | awk -F'JGFSEP' '{print $63}'`
set ROOTPE_GLC=`echo $xmlquery_data | awk -F'JGFSEP' '{print $64}'`
set ROOTPE_WAV=`echo $xmlquery_data | awk -F'JGFSEP' '{print $65}'`
set ROOTPE_ROF=`echo $xmlquery_data | awk -F'JGFSEP' '{print $66}'`
set PSTRID_CPL=`echo $xmlquery_data | awk -F'JGFSEP' '{print $67}'`
set PSTRID_ATM=`echo $xmlquery_data | awk -F'JGFSEP' '{print $68}'`
set PSTRID_LND=`echo $xmlquery_data | awk -F'JGFSEP' '{print $69}'`
set PSTRID_ICE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $70}'`
set PSTRID_OCN=`echo $xmlquery_data | awk -F'JGFSEP' '{print $71}'`
set PSTRID_GLC=`echo $xmlquery_data | awk -F'JGFSEP' '{print $72}'`
set PSTRID_WAV=`echo $xmlquery_data | awk -F'JGFSEP' '{print $73}'`
set PSTRID_ROF=`echo $xmlquery_data | awk -F'JGFSEP' '{print $74}'`
set NINST_ATM=`echo $xmlquery_data | awk -F'JGFSEP' '{print $75}'`
set NINST_LND=`echo $xmlquery_data | awk -F'JGFSEP' '{print $76}'`
set NINST_ICE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $77}'`
set NINST_OCN=`echo $xmlquery_data | awk -F'JGFSEP' '{print $78}'`
set NINST_GLC=`echo $xmlquery_data | awk -F'JGFSEP' '{print $79}'`
set NINST_WAV=`echo $xmlquery_data | awk -F'JGFSEP' '{print $80}'`
set NINST_ROF=`echo $xmlquery_data | awk -F'JGFSEP' '{print $81}'`
set PROFILE_PAPI_ENABLE=`echo $xmlquery_data | awk -F'JGFSEP' '{print $82}'`

setenv CASEROOT $CASEROOT
setenv CASE $CASE
setenv CASETOOLS $CASETOOLS
setenv CCSMROOT $CCSMROOT
setenv CIMEROOT $CIMEROOT
setenv LIBROOT $LIBROOT
setenv EXEROOT $EXEROOT
setenv OBJROOT $OBJROOT
setenv INCROOT $INCROOT
setenv SHAREDLIBROOT $SHAREDLIBROOT
setenv CASEBUILD $CASEBUILD
setenv DIN_LOC_ROOT $DIN_LOC_ROOT
setenv RUNDIR $RUNDIR
setenv LOGDIR $LOGDIR
setenv MACH $MACH
setenv GMAKE $GMAKE
setenv GMAKE_J $GMAKE_J
setenv NINST_BUILD $NINST_BUILD
setenv NINST_VALUE $NINST_VALUE
setenv SMP_BUILD $SMP_BUILD
setenv SMP_VALUE $SMP_VALUE
setenv BUILD_THREADED $BUILD_THREADED
setenv BUILD_STATUS $BUILD_STATUS
setenv COMP_INTERFACE $COMP_INTERFACE
setenv USE_ESMF_LIB $USE_ESMF_LIB
setenv COMPILER $COMPILER
setenv MPILIB $MPILIB
setenv DEBUG $DEBUG
setenv OS $OS
setenv MAX_TASKS_PER_NODE $MAX_TASKS_PER_NODE
setenv PES_PER_NODE $PES_PER_NODE
setenv COST_PES $COST_PES
setenv CCSM_ESTCOST $CCSM_ESTCOST
setenv PROJECT $PROJECT
setenv COMP_CPL $COMP_CPL
setenv COMP_ATM $COMP_ATM
setenv COMP_LND $COMP_LND
setenv COMP_ICE $COMP_ICE
setenv COMP_OCN $COMP_OCN
setenv COMP_GLC $COMP_GLC
setenv COMP_WAV $COMP_WAV
setenv COMP_ROF $COMP_ROF
setenv NTASKS_CPL $NTASKS_CPL
setenv NTASKS_ATM $NTASKS_ATM
setenv NTASKS_LND $NTASKS_LND
setenv NTASKS_ICE $NTASKS_ICE
setenv NTASKS_OCN $NTASKS_OCN
setenv NTASKS_GLC $NTASKS_GLC
setenv NTASKS_WAV $NTASKS_WAV
setenv NTASKS_ROF $NTASKS_ROF
setenv NTHRDS_CPL $NTHRDS_CPL
setenv NTHRDS_ATM $NTHRDS_ATM
setenv NTHRDS_LND $NTHRDS_LND
setenv NTHRDS_ICE $NTHRDS_ICE
setenv NTHRDS_OCN $NTHRDS_OCN
setenv NTHRDS_GLC $NTHRDS_GLC
setenv NTHRDS_WAV $NTHRDS_WAV
setenv NTHRDS_ROF $NTHRDS_ROF
setenv ROOTPE_CPL $ROOTPE_CPL
setenv ROOTPE_ATM $ROOTPE_ATM
setenv ROOTPE_LND $ROOTPE_LND
setenv ROOTPE_ICE $ROOTPE_ICE
setenv ROOTPE_OCN $ROOTPE_OCN
setenv ROOTPE_GLC $ROOTPE_GLC
setenv ROOTPE_WAV $ROOTPE_WAV
setenv ROOTPE_ROF $ROOTPE_ROF
setenv PSTRID_CPL $PSTRID_CPL
setenv PSTRID_ATM $PSTRID_ATM
setenv PSTRID_LND $PSTRID_LND
setenv PSTRID_ICE $PSTRID_ICE
setenv PSTRID_OCN $PSTRID_OCN
setenv PSTRID_GLC $PSTRID_GLC
setenv PSTRID_WAV $PSTRID_WAV
setenv PSTRID_ROF $PSTRID_ROF
setenv NINST_ATM $NINST_ATM
setenv NINST_LND $NINST_LND
setenv NINST_ICE $NINST_ICE
setenv NINST_OCN $NINST_OCN
setenv NINST_GLC $NINST_GLC
setenv NINST_WAV $NINST_WAV
setenv NINST_ROF $NINST_ROF
setenv PROFILE_PAPI_ENABLE $PROFILE_PAPI_ENABLE

source $CASEROOT/env_mach_specific
if ($status != 0) then
   echo "Error: problem sourcing env_mach_specific"; exit -2
endif 

set MODELS     = (         cpl         atm         lnd         ice         ocn         glc         wav          rof)	  
set COMPONENTS = (   $COMP_CPL   $COMP_ATM   $COMP_LND   $COMP_ICE   $COMP_OCN   $COMP_GLC   $COMP_WAV    $COMP_ROF)  
set NTASKS     = ( $NTASKS_CPL $NTASKS_ATM $NTASKS_LND $NTASKS_ICE $NTASKS_OCN $NTASKS_GLC $NTASKS_WAV  $NTASKS_ROF) 
set NTHRDS     = ( $NTHRDS_CPL $NTHRDS_ATM $NTHRDS_LND $NTHRDS_ICE $NTHRDS_OCN $NTHRDS_GLC $NTHRDS_WAV  $NTHRDS_ROF) 
set ROOTPE     = ( $ROOTPE_CPL $ROOTPE_ATM $ROOTPE_LND $ROOTPE_ICE $ROOTPE_OCN $ROOTPE_GLC $ROOTPE_WAV  $ROOTPE_ROF) 
set PSTRID     = ( $PSTRID_CPL $PSTRID_ATM $PSTRID_LND $PSTRID_ICE $PSTRID_OCN $PSTRID_GLC $PSTRID_WAV  $PSTRID_ROF) 
set NINST      = (          1   $NINST_ATM  $NINST_LND  $NINST_ICE  $NINST_OCN  $NINST_GLC  $NINST_WAV   $NINST_ROF)  




