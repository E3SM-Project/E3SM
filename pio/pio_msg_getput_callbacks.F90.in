        ierr = pio_get_var(file, varid, dvar)
     else
        ierr = pio_put_var(file, varid, dvar)
     end if
  case (TYPEINT)
     if(msg == pio_msg_getvar_0D) then
        ierr = pio_get_var(file, varid, ivar)
