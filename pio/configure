fi

if test -n "$ac_init_help"; then
  case $ac_init_help in
     short | recursive ) echo "Configuration of PIO 1.7.3:";;
   esac
  cat <<\_ACEOF

Optional Features:
  --disable-option-checking  ignore unrecognized --enable/--with options
  --disable-FEATURE       do not include FEATURE (same as --enable-FEATURE=no)
  --enable-FEATURE[=ARG]  include FEATURE [ARG=yes]
  --enable-pnetcdf        use pnetcdf (Uses PNETCDF_PATH) (default auto
                          detect)
  --enable-netcdf         use serial netcdf (Uses NETCDF_PATH) (default yes)
  --enable-netcdf4        use netcdf4 with HDF5 capability (Uses NETCDF_PATH)
