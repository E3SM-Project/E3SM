# The default design requirement for this library is to use the OS
# specific default REAL kind for all its REAL type variables, while
# supports both REAL and DOUPLE PRECISION kinds for its interfaces
# through the Fortran 90 generic interface feature. (Jing Guo)

# This module requires ../mpeu module

SHELL           = @SHELL@

_I              = @INCLUDEFLAG@../mpeu @INCLUDEPATH@

DEFS            = @MACHDEFS@

FPP		= @FPP@
FPPFLAGS	= @FPPFLAGS@

CC		= @CC@

FC		= @FC@
FCFLAGS         = @FCFLAGS@ @BIT64@ @REAL8@
F90FLAGS	= @F90FLAGS@


@FRULE@:
	$(FC) -c $(_I) $(DEFS) $(FCFLAGS) $*.F

@F90RULE@:
	$(FC) -c $(_I) $(DEFS) $(FCFLAGS) $(F90FLAGS) $*.F90

@FRULECPP@:
	$(FPP) $(FPPFLAGS) $(DEFS) $*.F $*.f
	$(FC) -c $(FCFLAGS) $(_I) $*.f
	$(RM) $*.f

@F90RULECPP@:
	$(FPP) $(FPPFLAGS) $(DEFS) $*.F90 $*.f90
	$(FC) -c $(FCFLAGS) $(F90FLAGS) $(_I) $*.f90
	$(RM) $*.f90

.SUFFIXES: .H .h .F90 .f90 .F .f .c .o

#.----------------------------------------------------------------------

