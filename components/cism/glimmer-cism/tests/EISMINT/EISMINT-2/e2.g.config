# configuration for the EISMINT-2 test-case

[EISMINT-2]

[grid]
# grid sizes
ewn = 61
nsn = 61
upn = 11
dew = 25000
dns = 25000

[options]
dycore = 0
temperature = 1
flow_law = 2
isostasy = 0
marine_margin = 3
evolution = 0
basal_water = 1
slip_coeff = 1
vertical_integration = 1

[time]
tend = 200000.
dt = 5.
ntem = 1.
nvel = 1.
niso = 1.

[parameters]
flow_factor = 1.
geothermal = -42.e-3
basal_tract_const = 1.e-3

[CF default]
title: EISMINT-2 Exp G
comment: forced upper kinematic BC

[CF output]
name: e2.g.nc
frequency: 1000
variables: thk bmlt btemp temp ubas vbas uvel vvel wvel wgrd acab diffu bwat

