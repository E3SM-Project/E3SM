# configuration for the EISMINT-2 test-case

[EISMINT-2]

[grid]
# grid sizes
ewn = 76
nsn = 76
upn = 11
dew = 20000
dns = 20000

[options]
dycore = 0
topo_is_relaxed = 1
temperature = 1
flow_law = 2
isostasy = 0
marine_margin = 3
evolution = 0
basal_water = 1
slip_coeff = 2
#sliding_law = 0
vertical_integration = 1

[time]
tend = 200000.
dt = 5.
ntem = 1.
nvel = 1.
niso = 1.

[parameters]
flow_factor = 1.
geothermal = -42.e-3
basal_tract_const = 1.e-3

[Erosion]

[Basic_Transport]

[isostasy]

[CF default]
title: transport, llfa
comment: erosion + transport

[CF output]
name: er-transport2.nc
frequency: 1000
variables: thk bmlt bwat topg erate erosion seds1 seds2 seds3 taux tauy seds2_max seds2_vx seds2_vy  slc relx

[CF input]
name: topo.nc
time: 1

