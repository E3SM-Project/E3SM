[ISMIP-HOM-TEST]
# Test from ISMIP-HOM higher-order test suite
# see: Pattyn et al., The Cryosphere, 2, 95â€“108, 2008.
# www.the-cryosphere.net/2/95/2008
# http://homepages.ulb.ac.be/~fpattyn/ismip/ismiphom.pdf

[grid]
upn = 11
ewn = 40    
nsn = 40
dew = 4000   
dns = 4000   

[time]
tstart = 0.
tend = 0.
dt = 1.
dt_diag = 1.
idiag = 10
jdiag = 10

[options]
dycore = 2                  # 1 = glam, 2 = glissade
flow_law = 0                # 0 = isothermal, 2 = temperature dependent
evolution = 3               # 3 = inc. remapping, 4 = FO upwind
temperature = 0             # 0 = set column to surf. air temp, 1 = prognostic, 2 = hold at init. values

[ho_options]
which_ho_babc = 4           # 4 = no-slip at bed; 5 = beta field from .nc input
which_ho_efvs = 2           # 2 = nonlinear eff. visc. w/ n=3
which_ho_sparse = 3         # 1 = SLAP GMRES, 3 = glissade parallel PCG, 4 = Trilinos for linear solver
which_ho_nonlinear = 0      # 0 = Picard, 1 = JFNK
which_ho_assemble_beta = 1  # 0=smoothed by FEM assembly, 1 = local approx.

[parameters]

[CF default]
comment = generated by runISMIPHOM.py
title = ISMIP-HOM Experiment 

[CF input]
name = ismip-hom.nc
time = 1

[CF output]
variables = uvel vvel uvel_extend vvel_extend topg thk usurf wvel velnorm
frequency = 1
name = ismip-hom.out.nc

