[ISMIP-HOM-TEST]
# Test B from ISMIP-HOM higher-order test suite
# see: Pattyn et al., The Cryosphere, 2, 95â€“108, 2008.
# www.the-cryosphere.net/2/95/2008

[grid]
upn = 11
ewn = 41
nsn = 41
dew = 4000   
dns = 4000   

[time]
tstart = 0.
tend = 0.
dt = 1.

[options]
dycore = 1              # SIA=0; 1st-order = 1
flow_law = 0            # temp dependent = 2; constant/uniform rate factor = 0
evolution = 3           # incremental remapping = 3
temperature = 0         # none = 0; prognostic = 1

[ho_options]
which_ho_babc = 4       # no-slip basal boundary conditions = 4        
which_ho_efvs = 2       # nonlinear eff. visc. = 2
which_ho_sparse = 1     # serial SLAP GMRES = 1; Trilinos = 4 
which_ho_nonlinear = 0  # Picard = 0; JFNK = 1

[parameters]
flow_factor = 1.

[CF default]
comment = generated by runISMIPHOM.py
title = ISMIP-HOM Experiment B

[CF input]
name = ishom.b.nc
time = 1

[CF output]
variables = uvel vvel tau_xz tau_yz
frequency = 1
name = ishom.b.out.nc


