!/PR2      NMX2   => WADATS(IMOD)%NMX2
!/PR2      NMY0   => WADATS(IMOD)%NMY0
!/PR2      NMY1   => WADATS(IMOD)%NMY1
!/PR2      NMY2   => WADATS(IMOD)%NMY2
!/PR2      NACT   => WADATS(IMOD)%NACT
!/PR2      NMXY   => WADATS(IMOD)%NMXY
!
!/PR3      NMX0   => WADATS(IMOD)%NMX0
