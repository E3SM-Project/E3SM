!/NL1                                 AWG7, AWG8, SWG1, SWG2, SWG3, SWG4,  &
!/NL1                                 SWG5, SWG6, SWG7, SWG8
!/NL1        REAL, POINTER         :: AF11(:)
!/NL1        LOGICAL               :: NLINIT
!
        INTEGER, POINTER      :: IAPPRO(:)
!/MPI        INTEGER               :: MPI_COMM_WAVE, MPI_COMM_WCMP,        &
