             IF (GTYPE .EQ. UNGTYPE) THEN
                 ALLOCATE( WADATS(IMOD)%ITER(NK,NTH), STAT=ISTAT )
                 CHECK_ALLOC_STATUS ( ISTAT )
               END IF
