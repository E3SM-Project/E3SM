<?xml version="1.0"?>

<!--
This defines any EAMxx specific CIME tests
-->

<config_test>

  <test NAME="EST">
    <DESC>Ensemble Statistical Test for climate reproducibility</DESC>
    <INFO_DBUG>1</INFO_DBUG>
    <DOUT_S>FALSE</DOUT_S>
    <CONTINUE_RUN>FALSE</CONTINUE_RUN>
    <STOP_OPTION>nmonths</STOP_OPTION>
    <STOP_N>12</STOP_N>
    <REST_OPTION>$STOP_OPTION</REST_OPTION>
    <REST_N>$STOP_N</REST_N>
    <HIST_OPTION>$STOP_OPTION</HIST_OPTION>
    <HIST_N>$STOP_N</HIST_N>
    <RESUBMIT>0</RESUBMIT>
  </test>

</config_test>
