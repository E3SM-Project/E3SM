<linoz_data_type            >INTERP_MISSING_MONTHS</linoz_data_type>

<!-- Turn off ozone dry deposition, as Linoz O3v2 and ozone are not separated for now. Need to turn on ozone dry deposition when interactive tropospheric chemistry is implemented -->
<drydep_list            >'H2O2', 'H2SO4', 'SO2'</drydep_list>

<!-- sim_year used for CLM datasets and SSTs forcings -->
<sim_year>2015-2100</sim_year>

<!-- CO2 rampping setting -->
<co2vmr>0.000001e-6</co2vmr>
<scenario_ghg>'RAMPED'</scenario_ghg>
<bndtvghg>atm/cam/ggas/GHG_GCAM_SSP245_Annual_Global_2015-2102_c20240712.nc</bndtvghg>

<!-- Time-variant CO2 fossil fuel emissions -->
<aircraft_datapath>atm/cam/chem/trop_mozart_aero/emis/GCAM_E3SM_SSP245/co2_emission_data_SSP245_GCAM/</aircraft_datapath>
<co2_readFlux_aircraft>.false.</co2_readFlux_aircraft>
<co2_readFlux_fuel>.false.</co2_readFlux_fuel>
