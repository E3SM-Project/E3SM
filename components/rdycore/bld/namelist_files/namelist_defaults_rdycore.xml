<?xml version="1.0"?>

<?xml-stylesheet type="text/xsl" href="namelist_defaults.xsl"?>

<namelist_defaults>

<!--
Values to use by default for creation of RDYCORE model namelists.
The element names are the same as the corresponding namelist
variables.  Values that depend on the model configuration use
attributes to express the dependency.  

The default filenames are given relative to the root directory
for the RDYCORE data in the E3SM distribution 
-->

<!-- Default rdycore namelist -->

<do_rdycore>.true.</do_rdycore> 
<rdycore_yaml_file>rdycore.yaml</rdycore_yaml_file>

