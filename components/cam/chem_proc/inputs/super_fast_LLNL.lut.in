              NO + O3 -> NO2 + O2                                       ;   3.000E-12, -1500
              HO2 + NO -> NO2 + OH                                      ;   3.500E-12, 250
              NO2 + OH + M -> HNO3                                      ;   1.800E-30, 3.00, 2.800E-11, 0.00, 0.6
              CH4 + OH -> CH3O2 + H2O                                   ;   2.450E-12, -1775
[usr_oh_co]   CO + OH -> HO2                                            
