<?xml version="1.0"?>

<namelist_defaults>

<!-- Ice nucleation mods-->
<use_hetfrz_classnuc>.true.</use_hetfrz_classnuc>
<use_preexisting_ice>.true.</use_preexisting_ice>
<hist_hetfrz_classnuc>.true.</hist_hetfrz_classnuc>
<micro_mg_dcs_tdep>.true.</micro_mg_dcs_tdep>
<microp_aero_wsub_scheme>2</microp_aero_wsub_scheme>

<!-- For Polar mods-->
<sscav_tuning>.true.</sscav_tuning>
<convproc_do_aer>.true.</convproc_do_aer>
<convproc_do_gas>.false.</convproc_do_gas>
<convproc_method_activate>2</convproc_method_activate>
<demott_ice_nuc>.true.</demott_ice_nuc>
<liqcf_fix>.true.</liqcf_fix>
<regen_fix>.true.</regen_fix>
<resus_fix>.true.</resus_fix>

<ssalt_tuning>.true.</ssalt_tuning>

<!-- For comprehensive history -->
<history_amwg>.true.</history_amwg>
<history_aerosol>.true.</history_aerosol>
<history_aero_optics>.true.</history_aero_optics>

<!-- Stratospheric ozone (Linoz) -->
<chlorine_loading_file      chem="linoz_mam3">atm/cam/chem/trop_mozart/ub/EESC_1850-2100_c090603.nc</chlorine_loading_file>
<chlorine_loading_fixed_ymd chem="linoz_mam3">20000101</chlorine_loading_fixed_ymd>
<chlorine_loading_type      chem="linoz_mam3">FIXED</chlorine_loading_type>
<linoz_data_cycle_yr        chem="linoz_mam3">2000</linoz_data_cycle_yr>
<linoz_data_file            chem="linoz_mam3">linoz1800-2100_2006jpl_climo_1.9x2.5_26L_extended.c160204.nc</linoz_data_file>
<linoz_data_path            chem="linoz_mam3">atm/cam/chem/trop_mozart/ub</linoz_data_path>
<linoz_data_type            chem="linoz_mam3">CYCLICAL</linoz_data_type>

<chlorine_loading_file      chem="linoz_mam4_resus">atm/cam/chem/trop_mozart/ub/EESC_1850-2100_c090603.nc</chlorine_loading_file>
