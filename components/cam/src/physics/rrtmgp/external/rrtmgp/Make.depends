#
# Gas concentrations
#
mo_gas_concentrations.o: mo_rte_kind.o mo_util_string.o mo_gas_concentrations.F90
#
# Gas optics
#
mo_gas_optics_kernels.o:       mo_rte_kind.o mo_gas_optics_kernels.F90
mo_gas_optics.o: mo_rte_kind.o mo_rrtmgp_constants.o mo_util_reorder.o \
  mo_util_string.o  mo_gas_optics_kernels.o \
  mo_gas_concentrations.o \
  mo_optical_props.o mo_source_functions.o \
	mo_gas_optics.F90
